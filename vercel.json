{
  "buildCommand": "npm run build",
  "outputDirectory": "dist/spa",
  "devCommand": "npm run dev",
  "installCommand": "pnpm install",
  "framework": null,
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "functions": {
    "netlify/functions/api.ts": {
      "runtime": "nodejs18.x"
    }
  }
}
